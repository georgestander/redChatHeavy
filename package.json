{
  "name": "chatjs",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "prebuild": "pnpm run check-env",
    "dev": "pnpm run check-env && pnpm exec node scripts/patch-react-jsx-dev-runtime.cjs && bash scripts/with-db.sh vite",
    "dev:inspect": "pnpm exec node scripts/patch-react-jsx-dev-runtime.cjs && NODE_OPTIONS=--inspect bash scripts/with-db.sh vite",
    "build": "vite build",
    "start": "wrangler dev",
    "prod": "pnpm run build && pnpm run start",
    "lint": "pnpm exec ultracite check",
    "format": "pnpm exec ultracite fix",
    "check-env": "pnpm exec tsx scripts/check-env.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "export VERCEL_ENV=production && bash scripts/with-db.sh pnpm exec drizzle-kit migrate",
    "db:backfill-parts": "npx tsx lib/db/backfill-parts.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:branch:use": "bash scripts/db-branch-use.sh",
    "db:branch:create": "bash scripts/db-branch-create.sh",
    "db:branch:delete": "bash scripts/db-branch-delete.sh",
    "db:branch:start": "bash -c 'N=${1:-dev-local}; pnpm db:branch:create $N && pnpm db:branch:use $N' --",
    "db:branch:stop": "bash -c 'N=${1:-dev-local}; pnpm db:branch:use main && pnpm db:branch:delete $N' --",
    "db:branch:list": "pnpm dlx neonctl branches list",
    "eval:dev": "evalite watch",
    "eval:serve": "evalite serve",
    "skiller:apply": "pnpm dlx skiller@latest apply",
    "test": "pnpm test:playwright && pnpm test:unit",
    "test:playwright": "export PLAYWRIGHT=True && pnpm exec playwright test --workers=4 --pass-with-no-tests",
    "test:unit": "vitest run",
    "test:types": "tsc --noEmit",
    "ai:devtools": "pnpm dlx @ai-sdk/devtools",
    "hooks:enable": "husky",
    "hooks:disable": "git config core.hooksPath .git/hooks",
    "fetch:models": "pnpm exec tsx scripts/fetch-models.ts && pnpm format",
    "chatjs:init": "pnpm exec tsx scripts/init.ts"
  },
